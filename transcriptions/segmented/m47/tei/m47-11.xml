            <zone points="609,3345 609,3386 860,3357 971,3374 994,3374 1114,3348 1155,3368 1167,3368 1228,3345 1257,3362 1272,3362 1371,3345 1558,3354 1622,3354 1689,3330 1730,3348 1925,3322 1949,3339 2042,3333 2042,3295 2036,3252 1925,3237 1873,3255 1803,3231 1794,3231 1727,3260 1727,3263 1680,3263 1639,3237 1563,3240 1563,3243 1540,3260 1453,3243 1415,3275 1353,3275 1301,3249 1292,3249 1237,3275 1193,3252 1184,3252 1091,3284 1082,3284 1006,3260 997,3260 930,3292 831,3292 773,3266 761,3266 761,3269 732,3295 732,3298 606,3298 609,3345" rendition="Line" xml:id="facs__eSc_line_1a026de6"/>
