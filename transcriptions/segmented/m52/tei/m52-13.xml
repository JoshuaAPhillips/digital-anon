            <zone points="928,3315 926,3344 1075,3346 1077,3346 1079,3346 1081,3346 1120,3332 1188,3346 1192,3346 1194,3346 1196,3346 1233,3332 1280,3346 1286,3348 1287,3348 1289,3348 1330,3346 1556,3334 1673,3348 1680,3348 1682,3348 1684,3348 1686,3348 1706,3338 1735,3348 1741,3350 1743,3350 1745,3352 1803,3352 1803,3350 1805,3350 1809,3348 1836,3336 1887,3348 1904,3352 1906,3352 1908,3352 1922,3348 1965,3338 2081,3338 2101,3350 2103,3350 2105,3350 2107,3350 2109,3350 2112,3350 2149,3338 2177,3348 2179,3348 2180,3348 2276,3340 2301,3350 2311,3354 2313,3354 2315,3354 2334,3350 2383,3340 2418,3350 2430,3354 2431,3354 2433,3354 2507,3350 2509,3325 2509,3286 2480,3276 2478,3276 2476,3276 2210,3284 2194,3284 2192,3284 2147,3272 2145,3272 2144,3272 1978,3282 1965,3282 1961,3282 1931,3272 1930,3272 1928,3272 1926,3272 1898,3282 1894,3282 1891,3282 1863,3270 1861,3270 1859,3270 1858,3270 1856,3270 1840,3280 1838,3280 1766,3270 1764,3270 1762,3270 1745,3280 1743,3280 1721,3280 1558,3268 1556,3268 1494,3276 1480,3278 928,3272 928,3315" rendition="Line" xml:id="facs__eSc_line_f32f6ac0" subtype="default"/>
