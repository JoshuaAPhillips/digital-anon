            <zone points="2470,3311 2341,3303 2339,3303 2338,3303 2316,3309 2295,3315 2244,3307 2180,3299 2178,3299 2176,3299 2155,3307 2133,3313 1863,3311 1845,3301 1830,3293 1828,3293 1826,3293 1824,3293 1810,3299 1795,3307 1454,3305 1410,3293 1384,3288 1382,3288 1314,3291 1188,3299 1128,3288 1100,3284 1098,3284 931,3284 931,3326 931,3354 1309,3354 1332,3361 1344,3365 1346,3365 1347,3365 1377,3363 1437,3356 1474,3363 1488,3367 1489,3367 1491,3367 1511,3365 1558,3358 1610,3367 1628,3369 1630,3369 1645,3367 1694,3360 1904,3371 1970,3375 1972,3375 1987,3373 2036,3365 2079,3375 2090,3377 2092,3377 2116,3375 2215,3367 2468,3381 2472,3350 2472,3348 2470,3311" rendition="Line" xml:id="facs__eSc_line_ba561c2a" subtype="default"/>
